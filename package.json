{
  "name": "starter-app-12",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "start:prod:local": "NODE_ENV=production mc-http-server --config=$(pwd)/env.json --use-local-assets",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data 'src/**/!(*.spec).js' --build-translations",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "compile-html:us": "mc-scripts compile-html --csp=$(pwd)/production-us.csp.json --config=$(pwd)/production-us.env.json --use-local-assets --transformer $(pwd)/config/transformer-now.js",
    "deploy:us": "yarn compile-html:us && now --local-config=production-us.now.json --target production"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@commercetools-frontend/actions-global": "13.11.0",
    "@commercetools-frontend/application-shell": "13.11.0",
    "@commercetools-frontend/application-shell-connectors": "13.11.0",
    "@commercetools-frontend/constants": "13.11.0",
    "@commercetools-frontend/i18n": "13.11.0",
    "@commercetools-frontend/mc-http-server": "13.11.0",
    "@commercetools-frontend/permissions": "13.11.0",
    "@commercetools-frontend/sdk": "13.11.0",
    "@commercetools-frontend/ui-kit": "9.9.0",
    "@emotion/babel-preset-css-prop": "^10.0.9",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-apollo": "2.5.6",
    "react-dom": "16.8.6",
    "react-intl": "2.9.0",
    "react-redux": "7.1.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.1"
  },
  "devDependencies": {
    "@commercetools-frontend/jest-preset-mc-app": "13.8.0",
    "@commercetools-frontend/mc-scripts": "13.11.0",
    "enzyme": "3.10.0",
    "jest": "24.8.0",
    "react-testing-library": "8.0.1"
  }
}
