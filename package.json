{
  "name": "cagcp",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "start:prod:local": "NODE_ENV=production mc-http-server --config=$(pwd)/env.json --use-local-assets",
    "compile-html:firebase": "cross-env FIREBASE_TARGET=cagcp mc-scripts compile-html --headers=$(pwd)/production-firebase.headers.json --config=$(pwd)/production-firebase.env.json --use-local-assets --transformer=$(pwd)/config/transformer-firebase.js",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data 'src/**/!(*.spec).js' --build-translations",
    "lint": "jest --projects jest.eslint.config.js jest.eslint-graphql.config.js jest.stylelint.config.js",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:graphql": "jest --config jest.eslint-graphql.config.js --passWithNoTests",
    "lint:css": "jest --config jest.stylelint.config.js",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "deploy:firebase": "yarn compile-html:firebase && firebase deploy"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@commercetools-frontend/actions-global": "15.1.4",
    "@commercetools-frontend/application-shell": "15.1.4",
    "@commercetools-frontend/application-shell-connectors": "15.1.4",
    "@commercetools-frontend/constants": "15.1.4",
    "@commercetools-frontend/i18n": "15.1.4",
    "@commercetools-frontend/mc-http-server": "15.1.4",
    "@commercetools-frontend/permissions": "^15.1.4",
    "@commercetools-frontend/sdk": "15.1.4",
    "@commercetools-frontend/ui-kit": "10.0.1",
    "@emotion/babel-preset-css-prop": "^10.0.9",
    "@types/react": "16.9.4",
    "apollo-utilities": "^1.3.2",
    "prop-types": "15.7.2",
    "react": "^16.10.2",
    "react-apollo": "^3.1.2",
    "react-dom": "16.8.6",
    "react-intl": "^3.3.2",
    "react-redux": "7.1.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.1"
  },
  "devDependencies": {
    "@commercetools-frontend/jest-preset-mc-app": "15.1.6",
    "@commercetools-frontend/mc-scripts": "15.1.6",
    "cross-env": "^6.0.3",
    "enzyme": "3.10.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-formatter-pretty": "2.1.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-graphql": "3.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.15.1",
    "eslint-plugin-react-hooks": "2.1.1",
    "jest": "24.8.0",
    "react-testing-library": "^8.0.1"
  }
}
